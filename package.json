{
  "name": "react-images-extended-2",
  "version": "1.1.0",
  "description": "A simple, responsive lightbox component for displaying an array of images with React.js with extended features",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "style": "dist/styles.css",
  "files": [
    "dist/"
  ],
  "author": "Isla May",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ryan-n-may/react-images-extended-2"
  },
  "dependencies": {
    "lucide-react": "0.536.0",
    "ts": "0.2.2",
    "typescript": "5.4.3",
    "react-pdf": "10.1.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react-pdf": "6.2.0",
    "@types/pdf-parse": "1.1.5",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.4.49",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.4.17"
